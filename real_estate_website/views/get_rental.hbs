</head>
<div class="jumbotron getSale_first">
            <nav class="navbar navbar-expand-lg navbar-light fixed-top navbartop">
<!--              <a class="navbar-brand" href="#">Navbar</a>-->
              <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>

              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
                  <li class="nav-item active">
                    <a class="nav-link cl" href="/" style="color: black;">Anasayfa</a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link cl" href="#" style="color: black;">Hakkimizda</a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link cl" href="/getSales" style="color: black;">Satilik</a>
                  </li>
                    <li class="nav-item">
                    <a class="nav-link cl" href="/getRental" style="color: black;">Kiralik</a>
                  </li>
                    <li class="nav-item">
                    <a class="nav-link cl" href="#" style="color: black;">Iletisim</a>
                  </li>
                </ul>
                    </div>
                </nav>
            </div>

<div class="container">
    <div class="row">
        {{#each rental}}
        <div class="col-12 col-xl-12 ads">
            <div class="row">
                <div class="col-12 col-xl-6 pics">
                    <div id="{{{_id}}}" class="carousel slide" data-ride="carousel">
                        
                       <div class="carousel-inner">
                          
                       <div class="carousel-item active">
 
                        <img class="d-block w-100 imgClass" src="../uploads/{{img.[0].filename}}" alt="First slide">
 
                        </div>
                        {{#eachOne img}}
                        <div class="carousel-item">
                        <img class="d-block w-100 imgClass" src="../uploads/{{filename}}" alt="Second slide">
                        </div>
                        {{/eachOne}}
                        </div>
                         <a class="carousel-control-prev" href="#{{{_id}}}" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#{{{_id}}}" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                        <div class="col-12">
                        <p data-toggle="modal" data-target=".{{{_id}}}">Click here to enlarge the pictures</p>
                        </div>
                    </div>
            </div>

                    <div class="modal fade {{{_id}}}" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
                    <div class="modal-dialog modal-lg">
                       
                        <div class="modal-content">

                            <div id="{{{_id}}}abc" class="carousel" data-ride="carousel">
                                <div class="carousel-inner modalPic">
                                    <div class="carousel-item active">
                                    <img class="d-block w-100 imgClass" src="../uploads/{{img.[0].filename}}" alt="First slide">
                                    </div>
                                    {{#eachOne img}}
                                    <div class="carousel-item">
                                    <img class="d-block w-100 imgClass" src="../uploads/{{filename}}" alt="Second slide">
                                        </div>
                                   {{/eachOne}}
                                </div>
                                    <a class="carousel-control-prev" href="#{{{_id}}}abc" role="button" data-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#{{{_id}}}abc" role="button" data-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </div>
                                
                        </div>
                        
                    </div>
                    </div>


                    

                
                 <div class="col-xl-6">
                    
                        <table class="table col-xl-12 tableInner">
                            <thead>
                                <tr>
                                <th scope="col">type</th>
                                <th scope="col">price</th>
                                <th scope="col">rooms</th>
                                <th scope="col">heat</th>
                                <th>City</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                <td>{{ type }}</td>
                                <td>{{ price }}</td>
                                <td>{{ rooms }}</td>
                                <td>{{ heatStyle }}</td>
                                <td>{{ city }}</td>
                                </tr>
                            </tbody>
                            <thead>
                            <tr>
                                <th>semt</th>
                                <th>age</th>
                                <th>floor</th>
                                <th>story</th>
                            </tr>
                            </thead>
                            <tbody>
                                <tr>
                                <td>{{ neighbor }}</td>
                                <td>{{ age }}</td>
                                <td>{{ floor }}</td>
                                <td>{{ totalStory }}</td>
                                </tr>
                            </tbody>
                            </table>
                    
                    <div class="col-xl-12 details">
                        <p>{{details}}</p>

                    </div>
                    
                </div>
        </div>
        </div>
        {{/each}}
</div>
</div>
</div>

